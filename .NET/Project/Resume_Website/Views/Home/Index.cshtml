@model Resume_Website.HomeViewModel

@{
    ViewBag.Title = "Basit Jamil";
    Layout = "~/Views/Shared/_NewTemplate.cshtml";
}
<!-- Start of Home Subpage -->
<section class="pt-page" data-id="home">
    <div class="section-inner start-page-content">
        <div class="page-header">
            <div class="row">
                <div class="col-sm-4 col-md-4 col-lg-4">
                    <div class="photo">
                        <img src="@Url.Content("~/Content/")profilepic1.PNG" alt="profile Picture">
                    </div>
                </div>

                <div class="col-sm-8 col-md-8 col-lg-8">
                    <div class="title-block">
                        <h1>@Resume_Service.ConfigService.Instance.GetValue("FullName").Value</h1>
                        <div class="owl-carousel text-rotation">
                            <div class="item">
                                <div class="sp-subtitle">@Resume_Service.ConfigService.Instance.GetValue("Designation").Value</div>
                            </div>
                            <div class="item">
                                <div class="sp-subtitle">@Resume_Service.ConfigService.Instance.GetValue("Language").Value</div>
                            </div>
                        </div>
                    </div>

                    <div class="social-links">
                        <a href="@Resume_Service.ConfigService.Instance.GetValue("Twitter").Value"><i class="fa fa-twitter"></i></a>
                        <a href="@Resume_Service.ConfigService.Instance.GetValue("Facebook").Value"><i class="fa fa-facebook"></i></a>
                        <a href="@Resume_Service.ConfigService.Instance.GetValue("Instagram").Value"><i class="fa fa-instagram"></i></a>
                    </div>
                </div>
            </div>
        </div>

        <div class="page-content">
            <div class="row">

                <div class="col-sm-6 col-md-6 col-lg-6">
                    <div class="about-me">
                        <div class="block-title">
                            <h3>About <span>Me</span></h3>
                        </div>
                        <p>@Resume_Service.ConfigService.Instance.GetValue("Aboutme").Value</p>
                    </div>
                    <div class="download-resume">
                        <a href="@Url.Action("Download","Home")" target="_blank" class="btn btn-secondary">Download Resume</a>
                    </div>
                </div>
                <div class="col-sm-6 col-md-6 col-lg-6">
                    <ul class="info-list">
                        <li><span class="title">Age</span><span class="value">@Resume_Service.ConfigService.Instance.GetValue("Age").Value</span></li>
                        <li><span class="title">Residence</span><span class="value">@Resume_Service.ConfigService.Instance.GetValue("Residence").Value</span></li>
                        <li><span class="title">Address</span><span class="value">@Resume_Service.ConfigService.Instance.GetValue("Address").Value</span></li>
                        <li><span class="title">e-mail</span><span class="value"><a href="@Resume_Service.ConfigService.Instance.GetValue("mailto").Value">@Resume_Service.ConfigService.Instance.GetValue("Email").Value</a></span></li>
                        <li><span class="title">Phone</span><span class="value">@Resume_Service.ConfigService.Instance.GetValue("Phone").Value</span></li>
                        <li><span class="title">Freelance</span><span class="value available">Available</span></li>
                    </ul>
                </div>

            </div>
        </div>
    </div>
</section>
<!-- End of Home Subpage -->
<!-- Resume Subpage -->
<section class="pt-page" data-id="resume">
    <div class="section-inner custom-page-content">
        <div class="page-header color-1">
            <h2>Resume</h2>
        </div>
        <div class="page-content">

            <div class="row">
                <div class="col-sm-6 col-md-6 col-lg-6">
                    <div class="block">
                        <div class="block-title">
                            <h3>Education</h3>
                        </div>

                        <div class="timeline">
                            <!-- Education 1 -->
                            <div class="timeline-item">
                                <h4 class="item-title">@Resume_Service.ConfigService.Instance.GetValue("Education1Degree").Value</h4>
                                <span class="item-period">@Resume_Service.ConfigService.Instance.GetValue("Education1Graduation").Value</span>
                                <span class="item-small">@Resume_Service.ConfigService.Instance.GetValue("Education1University").Value</span>
                                <p class="item-description">@Resume_Service.ConfigService.Instance.GetValue("Education1Description").Value</p>
                            </div>
                            <!-- / Education 1 -->
                            <!-- Education 2 -->
                            <div class="timeline-item">
                                <h4 class="item-title">@Resume_Service.ConfigService.Instance.GetValue("Education2Degree").Value</h4>
                                <span class="item-period">@Resume_Service.ConfigService.Instance.GetValue("Education2Graduation").Value</span>
                                <span class="item-small">@Resume_Service.ConfigService.Instance.GetValue("Education2University").Value</span>
                                <p class="item-description">@Resume_Service.ConfigService.Instance.GetValue("Education2Description").Value</p>
                            </div>
                            <!-- / Education 2 -->
                            <!-- Education 3 -->
                            <div class="timeline-item">
                                <h4 class="item-title">@Resume_Service.ConfigService.Instance.GetValue("Education3Degree").Value</h4>
                                <span class="item-period">@Resume_Service.ConfigService.Instance.GetValue("Education3Graduation").Value</span>
                                <span class="item-small">@Resume_Service.ConfigService.Instance.GetValue("Education3University").Value</span>
                                <p class="item-description">@Resume_Service.ConfigService.Instance.GetValue("Education3Description").Value</p>
                            </div>
                            @* Education 3*@
                        </div>
                    </div>
                </div>

                <div class="col-sm-6 col-md-6 col-lg-6">
                    <div class="block">
                        <div class="block-title">
                            <h3>Experience</h3>
                        </div>

                        <div class="timeline">
                            <!-- Experience 1 -->
                            <div class="timeline-item">
                                <h4 class="item-title">@Resume_Service.ConfigService.Instance.GetValue("WorkPlace1Designation").Value</h4>
                                <span class="item-period">@Resume_Service.ConfigService.Instance.GetValue("WorkPlace1Duration").Value</span>
                                <span class="item-small">@Resume_Service.ConfigService.Instance.GetValue("WorkPlace1Name").Value</span>
                                <p class="item-description"> @Resume_Service.ConfigService.Instance.GetValue("WorkPlace1Description").Value </p>
                            </div>
                            <!-- / Experience 1 -->
                            <!-- Experience 2 -->
                            <div class="timeline-item">
                                <h4 class="item-title">@Resume_Service.ConfigService.Instance.GetValue("WorkPlace2Designation").Value</h4>
                                <span class="item-period">@Resume_Service.ConfigService.Instance.GetValue("WorkPlace2Duration").Value</span>
                                <span class="item-small">@Resume_Service.ConfigService.Instance.GetValue("WorkPlace2Name").Value</span>
                                <p class="item-description"> @Resume_Service.ConfigService.Instance.GetValue("WorkPlace2Description").Value </p>
                            </div>
                            <!-- / Experience 2 -->
                            @*AddmoreIfyou want*@
                            <!-- Experience 3 -->
                            <!-- / Experience 3 -->
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                @foreach (var skill in Model.Skills)
                {
                    <div class="col-sm-6 col-md-6 col-lg-6">
                        <div class="block">
                            <div class="block-title">
                                <h3>@skill.Name &nbsp;<span>Skills</span></h3>
                            </div>
                            <div class="skills-info">
                                @foreach (var skilldetails in Model.SkillDetails.Where(x => x.SkillId == skill.SkillId).OrderBy(x => x.Name))
                                {
                                    <h4>@skilldetails.Name</h4>
                                    <div class="skill-container">
                                        <div class="@skilldetails.Percentage"></div>
                                    </div>
                                }
                            </div>
                        </div>
                    </div>
                }
            </div>

            <!-- Download Resume Button -->
            <div class="row">
                <div class="col-sm-12 col-md-12 col-lg-12">
                    <div class="block">
                        <div class="center download-resume">
                            <a href="@Url.Action("Download","Home")" class="btn btn-secondary">Download Resume</a>
                        </div>
                    </div>
                </div>
            </div>
            <!-- End of Download Resume Button -->

        </div>
    </div>
</section>
<!-- End of Resume Subpage -->
<!-- Services Subpage -->
<section class="pt-page" data-id="services">
    <div class="section-inner custom-page-content">
        <div class="page-header color-1">
            <h2>Services</h2>
        </div>
        <div class="page-content">
            <!-- My Services -->
            <div class="row">
                <div class="col-sm-12 col-md-12">
                    <div class="block-title">
                        <h3>My <span>Services</span></h3>
                    </div>
                </div>
                @foreach (var myservices in Model.MyServices)
                                    {
                    <div class="col-sm-6 col-md-3">
                        <div class="service-block">
                            <div class="service-info">
                                <!--<i class="service-icon fa fa-shopping-cart"></i>-->
                                <div class="service-image">
                                    <img src="@Url.Content("~/Content/NewTemplate/")@myservices.ImageURL" alt="Responsive Design" class="mCS_img_loaded">
                                </div>
                                <h4>@myservices.Name</h4>
                                <p>@myservices.Description</p>
                            </div>
                        </div>
                    </div>
                }
            </div>
            <!-- End of My Services -->
            <!-- Clients -->
            <div class="row">
                <div class="col-sm-12 col-md-12">
                    <div class="block-title">
                        <h3>Clients</h3>
                    </div>

                    @foreach (var client in Model.Clients)
                                {
                        <div class="col-sm-4 col-md-2 subpage-block">
                            <div class="client-block">
                                <a href="#" target="_blank"><img src="@Url.Content("~/Content/NewTemplate/")@client.Value" alt="image" class="mCS_img_loaded"></a>
                            </div>
                        </div>
                    }
                </div>
            </div>
            <!-- End of Clients -->
            <!-- Testimonials -->
            <div class="row">
                <div class="col-sm-12 col-md-12">
                    <div class="block-title">
                        <h3>Testimonials</h3>
                    </div>
                    <div class="testimonials owl-carousel block">
                        @foreach (var testimonials in Model.Testimonials)
                                {
                            <!-- Testimonial 1 -->
                            <div class="testimonial-item">
                                <!-- Testimonial Author -->
                                <div class="testimonial-credits">
                                    <!-- Picture -->
                                    <div class="testimonial-picture">
                                        <img src="@Url.Content("~/Content/NewTemplate/")@testimonials.ImageURL" alt="">
                                    </div>
                                    <!-- /Picture -->
                                    <!-- Testimonial author information -->
                                    <div class="testimonial-author-info">
                                        <p class="testimonial-author">@testimonials.Name</p>
                                        <p class="testimonial-firm">@testimonials.Company</p>
                                    </div>
                                </div>
                                <!-- /Testimonial author information -->
                                <!-- Testimonial Content -->
                                <div class="testimonial-content">
                                    <div class="testimonial-text">
                                        <p>" @testimonials.Description "</p>
                                    </div>
                                </div>
                                <!-- /Testimonial Content -->
                            </div>
                            <!-- End of Testimonial 1 -->
                        }
                    </div>
                </div>
            </div>
            <!-- End of Tesimonials -->
            <!-- Pricing -->
            <div class="row">
                <div class="col-sm-12 col-md-12">
                    <div class="block-title">
                        <h2 class="text-center"><span>Offers</span></h2>
                    </div>

                    <div class="lm-pricing row clearfix">
                        @foreach (var offer in Model.Offers)
                                {
                                    if (offer.Name == "Premium")
                                    {
                                <div class="lm-package-wrap col-md-4 highlight-col">
                                    <div class="lm-package">
                                        <div class="lm-heading-row">
                                            <span>@offer.Name</span>
                                        </div>
                                        <div class="lm-pricing-row">
                                            <span>@offer.Rate</span>
                                            <small>@offer.Duration</small>
                                        </div>

                                        <div class="lm-button-row">
                                            <a href="#contact" class="btn btn-primary">Get Services</a>
                                        </div>
                                        @foreach (var offerdetails in Model.OffersDetails.Where(x => x.OfferId == offer.Id))
                                        {
                                            <div class="lm-default-row">
                                                @offerdetails.Detail
                                            </div>
                                        }
                                        <div class="lm-default-row">
                                        </div>
                                    </div>
                                </div>
                            }
                            else
                            {
                                <div class="lm-package-wrap col-md-4 default-col">
                                    <div class="lm-package">
                                        <div class="lm-heading-row">
                                            <span>@offer.Name</span>
                                        </div>
                                        <div class="lm-pricing-row">
                                            <span>@offer.Rate</span>
                                            <small>@offer.Duration</small>
                                        </div>

                                        <div class="lm-button-row">
                                            <a href="#" target="_self" class="btn btn-primary">Get Services</a>
                                        </div>
                                        @foreach (var offerdetails in Model.OffersDetails.Where(x => x.OfferId == offer.Id))
                                        {
                                            <div class="lm-default-row">
                                                @offerdetails.Detail
                                            </div>
                                        }
                                        <div class="lm-default-row">
                                        </div>
                                    </div>
                                </div>
                            }

                        }
                    </div>
                </div>
            </div>
            <!-- End of Pricing -->
            <!-- End of Pricing -->
        </div>
    </div>
</section>
<!-- End of Services Subpage -->
