<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>


    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <meta name="description" content="Basit Jamil ASP .Net Developer" />
    <meta name="keywords" content="ASP MVC Developer" />
    <meta name="author" content="Basit Jamil" />
    <link rel="shortcut icon" href="@Url.Content("~/Content/")favicon.ico">

    <link rel="stylesheet" href="@Url.Content("~/Content/NewTemplate/")css/bootstrap.min.css" type="text/css">
    <link rel="stylesheet" href="@Url.Content("~/Content/NewTemplate/")css/normalize.css" type="text/css">
    <link rel="stylesheet" href="@Url.Content("~/Content/NewTemplate/")css/animate.css" type="text/css">
    <link rel="stylesheet" href="@Url.Content("~/Content/NewTemplate/")css/transition-animations.css" type="text/css">
    <link rel="stylesheet" href="@Url.Content("~/Content/NewTemplate/")css/owl.carousel.css" type="text/css">
    <link rel="stylesheet" href="@Url.Content("~/Content/NewTemplate/")css/magnific-popup.css" type="text/css">
    <link rel="stylesheet" href="@Url.Content("~/Content/NewTemplate/")css/main.css" type="text/css">

    <script src="@Url.Content("~/Content/NewTemplate/")js/jquery-2.1.3.min.js"></script>
    <script src="@Url.Content("~/Content/NewTemplate/")js/modernizr.custom.js"></script>


</head>
<body>

    <div class="preloader">
        <div class="preloader-animation">
            <div class="preloader-spinner">
            </div>
        </div>
    </div>
    <!-- /Loading animation -->

    <div id="page" class="page">
        <!-- Header -->
        <header id="site_header" class="header mobile-menu-hide">
            <div class="header-content">
                <div class="site-title-block mobile-hidden">
                    <div class="site-title">@Resume_Service.ConfigService.Instance.GetValue("FirstName").Value <span> @Resume_Service.ConfigService.Instance.GetValue("LastName").Value </span> </div>
                </div>

                <!-- Navigation -->
                <div class="site-nav">
                    <!-- Main menu -->
                    <ul id="nav" class="site-main-menu">
                        <li>
                            <a class="pt-trigger" href="#home" data-animation="62">Home</a><!-- href value = data-id without # of .pt-page. -->
                        </li>
                        <li>
                            <a class="pt-trigger" href="#resume" data-animation="62">Resume</a>
                        </li>
                        <li>
                            <a class="pt-trigger" href="#services" data-animation="62">Services</a>
                        </li>
                        @*   <li>
                                <a class="pt-trigger" href="#portfolio" data-animation="62">Portfolio</a>
                            </li>
                            <li>
                                <a class="pt-trigger" href="#blog" data-animation="62">Blog</a>
                            </li>*@
                        <li>
                            <a class="pt-trigger" href="#contact" data-animation="62">Contact</a>
                        </li>
                    </ul>
                    <!-- /Main menu -->
                </div>
                <!-- Navigation -->
            </div>
        </header>
        <!-- /Header -->
        <!-- Mobile Header -->
        <div class="mobile-header mobile-visible">
            <div class="mobile-logo-container">
                <div class="mobile-site-title">@Resume_Service.ConfigService.Instance.GetValue("FirstName").Value <span>@Resume_Service.ConfigService.Instance.GetValue("LastName").Value</span></div>
            </div>

            <a class="menu-toggle mobile-visible">
                <i class="fa fa-bars"></i>
            </a>
        </div>
        <!-- /Mobile Header -->
        <!-- Main Content -->
        <div id="main" class="site-main">
            <!-- Page changer wrapper -->
            <div class="pt-wrapper">
                <!-- Subpages -->
                <div class="subpages">
                    <div>
                        @RenderBody()
                    </div>
                </div>
            </div>

        </div>

        <footer>
            <div class="copyrights text-center">@Resume_Service.ConfigService.Instance.GetValue("CopyRight").Value</div>
        </footer>
    </div>

    <script type="text/javascript" src="@Url.Content("~/Content/NewTemplate/")js/bootstrap.min.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/NewTemplate/")js/pages-switcher.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/NewTemplate/")js/imagesloaded.pkgd.min.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/NewTemplate/")js/validator.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/NewTemplate/")js/jquery.shuffle.min.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/NewTemplate/")js/masonry.pkgd.min.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/NewTemplate/")js/owl.carousel.min.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/NewTemplate/")js/jquery.magnific-popup.min.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/NewTemplate/")js/jquery.hoverdir.js"></script>
    <!--<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=YOUR-API-KEY"></script>-->
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC7IuOflifct9IyL7WB-BcWFczbYE8cVWA" "></script>
    <script type="text/javascript" src="@Url.Content("~/Content/NewTemplate/")js/jquery.googlemap.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/NewTemplate/")js/main.js"></script>

</body>
</html>
